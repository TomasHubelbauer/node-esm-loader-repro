{
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": ".",
    "lib": [
      "ESNext"
    ],
    "module": "ESNext",
    "target": "ESNext",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "declarationMap": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules"
  ],
}
